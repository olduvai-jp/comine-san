{
  "name": "comine-san",
  "version": "1.0.0",
  "description": "run comfy-ui on the command line",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "comine-san": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "require": "./dist/lib/index.js",
      "import": "./dist/lib/index.js"
    },
    "./workflow": {
      "types": "./dist/lib/workflow/workflow.d.ts",
      "require": "./dist/lib/workflow/workflow.js",
      "import": "./dist/lib/workflow/workflow.js"
    },
    "./workflow/*": {
      "types": "./dist/lib/workflow/*.d.ts",
      "require": "./dist/lib/workflow/*.js",
      "import": "./dist/lib/workflow/*.js"
    },
    "./nodes/*": {
      "types": "./dist/lib/workflow/nodes/*.d.ts",
      "require": "./dist/lib/workflow/nodes/*.js",
      "import": "./dist/lib/workflow/nodes/*.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "require": "./dist/cli/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "lib": [
        "dist/lib/index.d.ts"
      ],
      "workflow": [
        "dist/lib/workflow/workflow.d.ts"
      ],
      "workflow/*": [
        "dist/lib/workflow/*.d.ts"
      ],
      "nodes/*": [
        "dist/lib/workflow/nodes/*.d.ts"
      ],
      "cli": [
        "dist/cli/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "docs",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "ts-node src/cli/index.ts",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/ws": "^8.5.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
