{
  "name": "comine-san",
  "version": "1.0.0",
  "description": "run comfy-ui on the command line",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "comine-san": "dist/cjs/cli/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.mjs"
    },
    "./lib": {
      "types": "./dist/types/lib/index.d.ts",
      "require": "./dist/cjs/lib/index.js",
      "import": "./dist/esm/lib/index.mjs"
    },
    "./workflow": {
      "types": "./dist/types/lib/workflow/workflow.d.ts",
      "require": "./dist/cjs/lib/workflow/workflow.js",
      "import": "./dist/esm/lib/workflow/workflow.mjs"
    },
    "./workflow/*": {
      "types": "./dist/types/lib/workflow/*.d.ts",
      "require": "./dist/cjs/lib/workflow/*.js",
      "import": "./dist/esm/lib/workflow/*.mjs"
    },
    "./nodes/*": {
      "types": "./dist/types/lib/workflow/nodes/*.d.ts",
      "require": "./dist/cjs/lib/workflow/nodes/*.js",
      "import": "./dist/esm/lib/workflow/nodes/*.mjs"
    },
    "./cli": {
      "types": "./dist/types/cli/index.d.ts",
      "require": "./dist/cjs/cli/index.js",
      "import": "./dist/esm/cli/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "lib": [
        "dist/types/lib/index.d.ts"
      ],
      "workflow": [
        "dist/types/lib/workflow/workflow.d.ts"
      ],
      "workflow/*": [
        "dist/types/lib/workflow/*.d.ts"
      ],
      "nodes/*": [
        "dist/types/lib/workflow/nodes/*.d.ts"
      ],
      "nodes/*/*": [
        "dist/types/lib/workflow/nodes/*/*.d.ts"
      ],
      "cli": [
        "dist/types/cli/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "docs",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.cjs.json && node scripts/build-esm.mjs",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "format": "prettier . --write --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "start": "ts-node src/cli/bin.ts",
    "test": "node scripts/test.mjs",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^20.14.0",
    "@types/ws": "^8.5.10",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.54.0"
  }
}
